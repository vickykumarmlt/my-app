
pipeline{
	agent any
	environment {
        	//mvnHome= tool "(name: 'Apache Maven 3.6.0', type: 'maven')+'/bin/mvn'"
	}
	tools 
	 {
	 maven 'Apache Maven 3.6.0'
	 } 
    
	
	stages{
		stage('SCM-Checkout'){
			steps{
				git 'https://github.com/vickykumarmlt/my-app'
			} }   		
		stage('Compile and package'){
			steps{
				sh "mvn clean package"				
			}
		}
	}

}
