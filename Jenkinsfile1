pipeline{
	agent any
	
	stages{
		stage('SCM-Checkout'){
			steps{
				git 'https://github.com/vickykumarmlt/my-app'
			} }   		
		stage('Compile and package'){
			steps{
				def mvnHome= tool (name: 'Apache Maven 3.6.0', type: 'maven')+'/bin/mvn'
				sh "${mvnHome} clean package"				
			}
		}
	}

}
